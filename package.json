{
  "name": "@satvik-s/s3-payload-client",
  "version": "0.0.2",
  "description": "Library to allow transferring payloads using AWS S3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf node_modules dist",
    "test": "echo no tests!",
    "build": "npm run clean && npm ci && tsc",
    "static-analysis": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "check-updates": "ncu",
    "make-updates": "ncu -u",
    "prepare": "husky install"
  },
  "keywords": [
    "typescript",
    "s3",
    "payloads"
  ],
  "author": "Satvik Shukla",
  "license": "MIT",
  "homepage": "https://github.com/satvik-s/s3-payload-client",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.235.0"
  },
  "devDependencies": {
    "@types/node": "18.11.9",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.2",
    "lint-staged": "13.0.3",
    "npm-check-updates": "16.4.1",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.4"
  },
  "lint-staged": {
    "*": "npm run prettier"
  }
}
